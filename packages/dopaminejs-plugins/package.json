{
    "name": "dopaminejs-plugins",
    "version": "1.0.0",
    "description": "Official plugins for DopamineJS - Howler.js audio, WebGL particles, and more",
    "type": "module",
    "main": "./dist/plugins.umd.js",
    "module": "./dist/plugins.mjs",
    "exports": {
        ".": {
            "import": "./dist/plugins.mjs",
            "require": "./dist/plugins.umd.js"
        },
        "./howler": "./dist/howler.mjs",
        "./webgl": "./dist/webgl.mjs",
        "./ecosystem": "./dist/ecosystem.mjs"
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "vite build",
        "test": "vitest"
    },
    "peerDependencies": {
        "dopaminejs": "^2.0.0"
    },
    "keywords": [
        "dopaminejs-plugin",
        "game",
        "audio",
        "particles",
        "webgl",
        "howler"
    ],
    "author": "BaryoDev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/BaryoDev/dopaminejs.git",
        "directory": "packages/dopaminejs-plugins"
    }
}